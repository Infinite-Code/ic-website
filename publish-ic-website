date
BASE_DIR=`dirname ${0}`

echo Preparing files for sync..

${BASE_DIR}/gzip-all

echo Syncing infinite-code.com...

export AWS_CALLING_FORMAT=SUBDOMAIN
export S3SYNC_MIME_TYPES_FILE=~/Scripts/s3sync/mime.types
AWS_BIN_PATH=/Users/cheeming/virtualenv/aws-cli/bin
${AWS_BIN_PATH}/aws s3 sync ${BASE_DIR}/www_gzip/ s3://infinite-code.com --delete --acl public-read --content-encoding gzip

date
